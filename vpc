terraform {
  required_version = "~> 1.14.0"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "3.60.0"
    }
  }
}

provider "aws" {
  region = "us-east-1"
}

resource "aws_vpc" "vpc-us-east-1" {
  cidr_block = "10.1.0.0/16"

  tags = {
    Name = "vpc-us-east-1"
  }
}
